{
    "configurations": [
        {
            "name": "Cloud Run: Run/Debug Locally",
            "type": "cloudcode.cloudrun",
            "request": "launch",
            "build": {
                "docker": {
                    "path": "Dockerfile",
                    "target": "holitime",
                    "buildArgs": {},
                    "secrets": []
                }
            },
            "image": "holitime",
            "service": {
                "name": "holitime",
                "containerPort": 8080,
                "resources": {
                    "limits": {
                        "memory": "2GB"
                    }
                }
            },
            "target": {
                "minikube": {}
            },
            "watch": true
        }
    ]
}